{
  "name": "vue-webpack-4",
  "version": "1.1.0",
  "description": "A Vuejs template using Webpack 4",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --progress --watch --config build/webpack.config.dev.js",
    "prod": "NODE_ENV=production webpack --config build/webpack.config.prod.js",
    "lint": "eslint --ext .ts,.js,.vue src lib",
    "lint:fix": "eslint --ext .ts,.js,.vue src lib --fix",
    "test:unit": "jest --config test/jest.config.js",
    "test": "npm run lint && npm run test:unit",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand --config test/jest.config.js"
  },
  "keywords": [
    "Vue",
    "Webpack 4"
  ],
  "author": "Happy",
  "license": "MIT",
  "dependencies": {
