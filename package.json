    "nprogress": "^0.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "throttle-debounce": "^2.1.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "@testing-library/vue": "^3.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.1",
